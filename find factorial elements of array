//to find factorial of individual elements of array
#include<stdio.h>
void main()
{
	int a[30],i,j,n,fact;
	printf("enter no of elements:");
	scanf("%d",&n);
	printf("enter numbers:");
	for(i=0;i<n;i++)
	scanf("%d",&a[i]);
	for(i=0;i<n;i++)
	{
		fact=1;
		j=a[i];
		while(j>1)
		{
			fact*=j;
			j--;
		}
	printf("factorial of %d is %d\n",a[i],fact);
	}
}
