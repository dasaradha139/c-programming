#include<stdio.h>
void main()
{
	int a[30][30],i,j,row,col,sum=0,sumrow;
	printf("enter no of rows:");
	scanf("%d",&row);
	printf("enter no of coloms:");
	scanf("%d",&col);
	printf("enter array elements");
	for(i=0;i<row;i++)
		for(j=0;j<col;j++)
		   scanf("%d",&a[i][j]);
	if(row==col)
	{
		i=0;
		j=col-1;
	while(i<row&&j>=0)
	{
		sum+=a[i][j];
		i++;
		j--;
	}
	printf("sum of digits of right diagnol is %d",sum);
    }
    else 
    printf("left diagnol not exist");
}
