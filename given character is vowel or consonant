//Write a C program to check if the given character is vowel or constant
#include<stdio.h>
void main()
{
 int ch;
 scanf("%c",&ch);
 if(ch=='A'||ch=='E'||ch=='I'||ch=='O'||ch=='U'||ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u')
 printf("character is vowel");
 else
 printf("character is consonant");
}
