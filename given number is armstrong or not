#include<stdio.h>
#include<math.h>
void main()
{
	int n,r,sum=0,power=0,m;
	scanf("%d",&n);
	m=n;
	int temp=n;
	while(m>0)
	{
		m=m/10;
		power++;
	}
	while(n>0)
	{
		r=n%10;
		sum+=pow(r,power);
		n=n/10;
	}
	if(sum==temp)
	printf("%d is an armstrong",sum);
	else
	printf("%d is not an armstrong",sum);

}
